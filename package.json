{
  "name": "@maboiteaspam/console.md",
  "version": "1.0.0",
  "description": "The missing console.md(msg) to print markdown with the nodejs console object",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dcheck": "npm outdated --depth=0",
    "patch": "npm version patch -m \"patch %s\"",
    "minor": "npm version minor -m \"minor %s\"",
    "major": "npm version major -m \"major %s\"",
    "preversion": "echo \"npm test: undefined\" && npi --explicit",
    "version": "echo \"npm run build: undefined\"",
    "postversion": "git push && git push --tags && npm run public",
    "public": "npm publish --access=public"
  },
  "keywords": [
    "console",
    "markdown"
  ],
  "author": "maboiteaspam",
  "license": "WTF",
  "dependencies": {
    "marked": "^0.3.5",
    "marked-terminal": "^1.6.1"
  }
}
